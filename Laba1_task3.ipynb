    "def kripta(g, q, X, Y):\n",
    "    # искомое 2 * a = X mod q\n",
    "    a = int((X + q) / 2)\n",
    "    b = int((Y + q) / 2)\n",
    "    K1 = (a * Y) % q\n",
    "    K2 = (b * X) % q\n",
    "    return K1, K2"
   ]
  },
